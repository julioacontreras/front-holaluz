{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "strict": true,
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "forceConsistentCasingInFileNames": true,
      "useDefineForClassFields": true,
      "sourceMap": true,
      "baseUrl": ".",
      "types": [],
      "paths": {
        "@/*": [
          "src/*"
        ]
      },
      "lib": [
        "esnext",
        "dom",
        "dom.iterable",
        "scripthost"
      ]
    },
    "include": [
      "src/**/*.ts",
      "src/**/*.vue"
    ],
    "exclude": [
      "src/main.js",
      "node_modules"
    ]
  }
  